#include<iostream>
#include<iomanip>
using namespace std;
int main () {
    int num;
    cout<<"Enter The Number : "<<endl;
    cin>>num;

    if(num % 4 == 0 && num  % 100 != 0 || num % 400 == 0){
        cout<<"The Year "<<num<<" is a leap year";
    }
    else {
        cout<<"The Year "<<num<<" is not a leap year";
    }



    return 0;
}
