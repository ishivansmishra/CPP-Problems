#include <iostream>
#include <iomanip>

using namespace std;

int main()
{
    int marks;
    cout << "Enter The your Marks : ";
    cin >> marks;

    if (marks >= 90 && marks <= 100)
    {
        cout << "You have get A grade";
    }
    else if (marks >= 80 && marks < 90)
    {
        cout << "You Have get B grade";
    }
    else if (marks >= 70 && marks < 80)
    {
        cout << "You Have get C grade";
    }
    else if (marks >= 60 && marks < 70)
    {
        cout << "You Have get D grade";
    }
    else if (marks >= 50 && marks < 60)
    {
        cout << "You Have get E grade";
    }
    else if (marks >= 0 && marks < 50)
    {
        cout << "You Have get F grade";
    }
    else
    {
        cout << "Invalid";
    }

    return 0;
}
