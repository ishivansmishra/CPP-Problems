#include <iostream>
#include <iomanip>

using namespace std;

int main()
{

    int n;
    cout << "Enter The Number : " << endl;
    cin >> n;

    int b;
    cout << "Enter The number you want to break : " << endl;
    cin >> b;

    for (int i = 0; i <= n; i++)
    {
        if (i == b)
        {
            break;
        }
        cout << i << endl;
    }

    return 0;
}
