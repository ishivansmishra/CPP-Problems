#include <iostream>
#include <iomanip>

using namespace std;

int main()
{
    int age;
    cout << "Enter Your Age : " << endl;
    cin >> age;

    if (age > 18)
    {
        cout << "Now You are Adult" << endl;
    }
    else if (age == 18)
    {
        cout << "You are Teen but tends to adult " << endl;
    }
    else
    {
        cout << "You are a minor";
    }

    return 0;
}
