#include<iostream>

using namespace std;


int main () {
    int n;
    cout<<"Enter The Size : ";
    cin>>n;

    int arr[n];
    cout<<"Enter the element : ";
    for(int i = 0;i<n;i++){
        cin>>arr[i];
    }

    int index = -1;
    int x;
    cout<<"Enter the number you search : ";
    cin>>x;

    for(int i = 0;i<n;i++){
        if(arr[i]==x){
            index = i;
        }
    }
   cout<<index;


    return 0;
}
