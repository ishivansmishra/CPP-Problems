#include <iostream>

using namespace std;

int main()
{
    int n;
    cout << "Enter The Number : " << endl;
    cin >> n;

    int arr[n];
    cout << "Enter The Array elements : " << endl;
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    cout << "Reverse The Array Elements : " << endl;

    for (int i = n - 1; i >= 0; i--)
    {
        cout << arr[i] << "\t";
    }

    return 0;
}
