#include <iostream>

using namespace std;
int main()
{
    int n;
    cout << "enter The Size : ";
    cin >> n;

    int arr[n];
    cout << "Enter The elment : ";
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    for (int i = 0; i < n; i += 2)
    {
        if (i + 1 < n)
        {
            swap(arr[i], arr[i + 1]);
        }
    }
    for (int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }

    return 0;
}
